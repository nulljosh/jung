# Built-in functions

# len
project len("hello")
project len([1, 2, 3])
project len([])

# str
project str(42)
project str(true)
project str(null)

# int
project int(3.7)
project int("42")

# type
project type(42)
project type("hello")
project type(true)
project type(null)
project type([1, 2])

# range
project range(5)
project range(2, 5)

# push / pop
perceive arr = [1, 2, 3]
push(arr, 4)
project arr
project pop(arr)
project arr

# split / join
perceive parts = split("a-b-c", "-")
project parts
project join(parts, ", ")

# slice (string)
project slice("hello world", 0, 5)
project slice("hello world", 6)

# slice (array)
project slice([10, 20, 30, 40, 50], 1, 3)

# keys / values / has
perceive obj = {name: "carl", age: 65}
project has(obj, "name")
project has(obj, "missing")

# math
project abs(-5)
project abs(5)
project floor(3.7)
project ceil(3.2)
project round(3.5)
project round(3.4)
project min(3, 7)
project max(3, 7)
project sqrt(16)
project pow(2, 10)

# sort / reverse
project sort([3, 1, 4, 1, 5])
project reverse([1, 2, 3])
